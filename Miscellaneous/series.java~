//OUTPUT: 3, 45, 9, 38, 27, 31, 81, 24, 243, 17
public class series{
    public static void main(String[] args){
        int count  = 1;
        int square_base = 3;
        int normal_term  = 45;
        int i = 1;
        while(i <= 10){
            System.out.print(square_base);
            if(i%2 != =){
                System.out.print(", ");
            }
            System.out.print(normal_term);
            if(i%2 == 0 && i != 10){
                System.out.print(", ");
            }
            count++;
            square_base = Math.pow(square_base, count);
            normal_term = normal_term - 7;
            i++
        }
    }
}