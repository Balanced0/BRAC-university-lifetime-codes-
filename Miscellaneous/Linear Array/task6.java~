//Remove Duplicates from Array
import java.util.Scanner;
public class task6{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int arr[] = new int[n];
        for(int i = 0; i < n; i++){
            arr[i] = sc.nextInt();
        }
        boolean flag = false;
        int arr1[];
        for(int i = 0; i < n; i++){
            int count = 0;
            for(int j = 0; j < i; j++){
                if(arr[i] == arr[j]){
                    count++;
                }
            }
            if(count == 2){
                flag = true;
            }
            if(flag != true){
                arr1[]
            }
        }
        System.out.println(flag);
    }
}