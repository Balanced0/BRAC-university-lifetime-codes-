import java.util.Scanner;
public class loops4{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter a positive integer");
        int a = sc.nextInt();
        int mul = 1;
        for(int i = 1; i <= a; i++){
            int factorial = mul * i;
            System.out.println(factorial);
        }
    } 
}