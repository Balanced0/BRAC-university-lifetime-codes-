//Arrays + Methods
//Task 10
import java.util.Scanner;
public class task11{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        double t = convertToCm(n);
        System.out.println(t + " cm");
    }
    
    public static double convertToCm(int n){
        double cm = 2.54 * n;
        return cm;
    }
}